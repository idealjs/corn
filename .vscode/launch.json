{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest Tests Reactive",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/reactive",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "--runInBand",
        "--roots=${workspaceRoot}/packages/reactive"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    },
    {
      "name": "Debug Jest Tests Reactive useEffect",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/reactive",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "--runInBand",
        "--roots=${workspaceRoot}/packages/reactive",
        "--filter=",
        "useEffect.test.ts"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    },
    {
      "name": "Debug Jest Tests Scheduler",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/reactive",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "--runInBand",
        "--roots=${workspaceRoot}/packages/reactive",
        "--filter=",
        "scheduler.test.ts"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    }
  ]
}
